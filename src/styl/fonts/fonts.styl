@font-face {
	font-family: 'Helvetica Neue';
	src: url('../static/fonts/helvetica-neue/HelveticaNeue.eot');
	src: url('../static/fonts/helvetica-neue/HelveticaNeue.eot?#iefix') format('embedded-opentype'),
		url('../static/fonts/helvetica-neue/HelveticaNeue.woff') format('woff'),
		url('../static/fonts/helvetica-neue/HelveticaNeue.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

